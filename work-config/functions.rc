#----Functions-------------
function countfiles() {
  readonly filetype=${1:?"Please provide a filetype"}
  find . -type f -name \*.$filetype | wc -l
}

function lsgrep() {
  ls | grep $1
}

function awsls() {
  aws s3 ls s3://vuka-ingest-manager/$1
}

function awscp() {
  aws s3 cp $1 s3://vuka-ingest-manager/$2
}